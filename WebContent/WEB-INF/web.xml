<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <listener>
    <listener-class>utils.SystemStartup</listener-class>
  </listener>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>servlets.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Fachschaften</servlet-name>
    <servlet-class>servlets.Fachschaften</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteEvent</servlet-name>
    <servlet-class>servlets.DeleteEvent</servlet-class>
  </servlet>  
  <servlet>
    <servlet-name>Event</servlet-name>
    <servlet-class>servlets.Event</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Fachschaften</servlet-name>
    <url-pattern>/fachschaft</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>Event</servlet-name>
    <url-pattern>/event</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>DeleteEvent</servlet-name>
    <url-pattern>/delete</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>